{
  "ci": {
    "collect": {
      "url": [
        "https://https-github-com-absulysuly-letsdoittonight-8iitp7ver.vercel.app"
      ],
      "startServerCommand": "",
      "settings": {
        "preset": "desktop"
      }
    },
    "assert": {
      "assertions": {
        "categories:performance": ["error", { "minScore": 0.9 }],
        "largest-contentful-paint": ["error", { "maxNumericValue": 2000, "aggregationMethod": "optimistic" }],
        "cumulative-layout-shift": ["error", { "maxNumericValue": 0.1, "aggregationMethod": "optimistic" }],
        "experimental-interaction-to-next-paint": ["error", { "maxNumericValue": 150, "aggregationMethod": "optimistic" }]
      }
    },
    "upload": {
      "target": "filesystem",
      "outputDir": ".lighthouseci"
    }
  }
}
